{
  "github": {
    "silent": true
  },
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/reference/:path*",
      "destination": "/apis/:path*",
      "permanent": false
    },
    {
      "source": "/tips/controlled-input-null-value.html",
      "destination": "/docs/forms.html#controlled-input-null-value",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/fonts/(.*).woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
